{
  "manifest_version": 2,
  "name": "Уткулятор",
  "version": "1.0.25",
  "description": "ᕕ( ՞ ᗜ ՞ )ᕗ",
  "icons": {
    "16": "./images/icon16.png",
    "48": "./images/icon48.png",
    "128": "./images/icon128.png"
  },

  "background": {
    "scripts": [
      "./src/scripts/background.js"
    ]
  },

  "page_action": {
    "default_title": "Кря!",
    "default_icon": "./images/icon16.png",
    "default_popup": "./src/stats/popup.html"
  },

  "content_scripts": [
    {
      "matches": [
        "https://www2.kufar.by/*",
        "http://egr.gov.by/egrn/*"
      ],
      "js": [
        "./src/public/bundle.js",
        "./src/scripts/egrscript.js"
      ]
    }
  ],

  "options_ui": {
    "page": "./src/options/options.html",
    "chrome_style": true
  },

  "permissions": [
    "storage",
    "declarativeContent",
    "https://www2.kufar.by/*",
    "tabs"
  ]
}
